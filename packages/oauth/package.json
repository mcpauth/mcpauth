    "drizzle-orm": "*",
    "uuid": "*",
    "pg": "*"
  },
  "devDependencies": {
    "@types/node": "latest",
    "@types/pg": "^8.11.5",
    "@types/react": "latest",
    "@types/uuid": "^9.0.8",
    "drizzle-orm": "^0.30.0",
    "pg": "^8.11.5",
    "prisma": "latest",
    "typescript": "latest"
  },
  "exports": {
    ".": {
