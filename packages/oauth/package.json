{
  "name": "@tmcp/oauth",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rm -rf ./dist && tsc -b tsconfig.json --force",
    "dev": "tsc -w"
  },
  "dependencies": {
    "@node-oauth/oauth2-server": "^5.2.0",
    "bcryptjs": "^3.0.2",
    "jose": "^5.6.3"
  },
  "peerDependencies": {
